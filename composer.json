{
    "name": "kevinpirnie/theme-framework",
    "description": "A modern WordPress parent theme framework with CSS framework selection",
    "type": "wordpress-theme",
    "version": "1.0.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Pirnie",
            "email": "iam@kevinpirnie.com",
            "role": "Developer"
        }
    ],
    "funding": [
        {
            "type": "paypal",
            "url": "https://www.paypal.biz/kevinpirnie"
        },
        {
            "type": "ko-fi",
            "url": "https://ko-fi.com/kevinpirnie"
        }
    ],
    "require": {
        "php": ">=8.4",
        "kevinpirnie/kpt-wpfieldframework": ">=1"
    },
    "autoload": {
        "classmap": [
            "work/classes",
            "work/modules"
        ],
        "files": [
            "work/kpf-main.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true,
        "apcu-autoloader": true,
        "sort-packages": true
    }
}